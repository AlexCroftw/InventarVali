@model List<Autovehicule>
<div class="card shadow border-0 my-4">
    <div class="card-header bg-secondary bg-gradient m-lg-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">Autovehicule List</h2>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
    </div>
    <div class="row pb-3">
       <div class="col-12 text-center">
            <a asp-controller="Autovehicule" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-square-fill"></i> Create New Autovehicule
            </a>
       </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Type
                </th>
                <th>
                    License Plate
                </th>
                <th>
                    VIN Number
                </th>
                <th>
                    Insurence Start Date
                </th>
                <th>
                    ITP
                </th>
                <th>
                    ITP Expiration Date
                </th>
                <th>
                    Insurance Expiration Date
                </th>
                <th>
                    Vinieta
                </th>
                <th>
                    Vinieta Expiration Date
                </th>
                <th>
                    Image
                </th>
                <th>
                    Employee
                </th>
                <th>

                </th>
            </tr>
        </thead>
        
    </table>

</div>

@section Scripts {
    <script src="~/js/autovehicule.js"></script>
}